{
    "manifest_version": 3,
    "name": "Workday Dark Mode",
    "version": "1.0",
    "description": "A dark mode theme for Workday.",
    "content_scripts": [
        {
            "matches": ["*://*.myworkday.com/*"],
            "js": ["content.js"],
            "css": ["dark-mode-style.css"]
        }
    ]
}